{
  "name": "beastbytes/router-register",
  "description": "Console command to create and register routes from attributes",
  "keywords": [
    "attributes",
    "router",
    "routes",
    "yii3"
  ],
  "type": "project",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "chris",
      "email": "email@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "ext-mbstring": "*",
    "symfony/console": "^6.0",
    "yiisoft/dummy-provider": "^1.0",
    "yiisoft/files": "^2.0",
    "yiisoft/router": "^4.0",
    "yiisoft/yii-console": "^2.0"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.0",
    "phpunit/phpunit": "^10.0",
    "rector/rector": "^1.0",
    "vimeo/psalm": "~5.26",
    "yiisoft/security": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "BeastBytes\\Router\\Register\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BeastBytes\\Router\\Register\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "config-plugin-options": {
      "source-directory": "config"
    },
    "config-plugin": {
      "di-console": "di-console.php",
      "params": "params.php"
    }
  },
  "config": {
    "sort-packages": true
  }
}
